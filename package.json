{
  "name": "hrmadminportal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "package": "pkg . --out-path dist --targets node18-win-x64"
  },
  "devDependencies": {
    "@iconify/svelte": "^2.2.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.2",
    "svelte": "^3.49.0",
    "vite": "^3.1.0",
    "pkg": "^5.7.0"  
  },
  "dependencies": {
    "axios": "^0.27.2",
    "date-fns": "^2.29.3",
    "history": "^5.3.0",
    "quill": "^1.3.6",
    "svelte-navigator": "^3.2.2"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "public/**/*",
    "targets": [
      "node18-win-x64"
    ],
    "output": "dist/my-svelte-app.exe"
  },
  "engines": {
    "node": ">=14"
